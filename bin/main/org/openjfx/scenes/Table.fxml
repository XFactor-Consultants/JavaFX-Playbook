<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.cell.* ?>
<?import javafx.collections.* ?>
<?import openjfx.* ?>


<TableView fx:id="table" VBox.vgrow="ALWAYS" maxHeight="Infinity" maxWidth="Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="300.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.openjfx.controllers.TableController">
   <columnResizePolicy>
        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
    </columnResizePolicy>
   <columns>
        <TableColumn prefWidth="200.0" text="Time">
            <cellValueFactory>
                <PropertyValueFactory property="dt"/>
            </cellValueFactory>
        </TableColumn>
        <TableColumn prefWidth="200.0" text="Temperature (F)" sortable="false">
            <cellValueFactory>
                <PropertyValueFactory property="temp"/>
            </cellValueFactory>
        </TableColumn>
   </columns>
   <items>
    <FXCollections fx:factory="observableArrayList">

    </FXCollections>
   </items>
</TableView>
